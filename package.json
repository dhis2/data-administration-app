{
  "name": "data-administration-app",
  "version": "31.0.0",
  "private": true,
  "devDependencies": {
    "@dhis2/cli-style": "^3.2.2",
    "@dhis2/d2-i18n-extract": "^1.0.8",
    "@dhis2/d2-i18n-generate": "^1.1.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "husky": "^2.3.0",
    "jest-enzyme": "^7.0.2"
  },
  "dependencies": {
    "@dhis2/app-runtime": "^2.0.3",
    "@dhis2/d2-i18n": "^1.0.6",
    "@dhis2/d2-ui-header-bar": "^1.1.7",
    "@dhis2/ui-core": "^4.1.1",
    "@dhis2/ui-widgets": "^2.0.5",
    "classnames": "^2.2.5",
    "d2": "^28.3.0",
    "d2-ui": "^29.0.18",
    "d3-color": "^1.0.3",
    "loglevel": "^1.6.0",
    "material-design-icons-iconfont": "^5.0.1",
    "material-ui": "^0.20.0",
    "prop-types": "^15.6.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.1",
    "rxjs": "^5.5.4",
    "typeface-roboto": "^0.0.75"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "npm run extract-pot && d2-i18n-generate -n data-admin-app -p ./i18n/ -o ./src/locales/",
    "lint": "d2-style js check --all",
    "prestart": "npm run localize",
    "prebuild": "npm run localize",
    "pretest": "npm run localize"
  },
  "husky": {
    "hooks": {
      "commit-msg": "d2-style commit check",
      "pre-commit": "d2-style js apply"
    }
  }
}
