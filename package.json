{
  "name": "dhis2-maintenance-data-admin-app",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "chai": "^4.1.2",
    "cucumber": "^3.2.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-config-dhis2": "^3.0.7",
    "i18next-conv": "^6.0.0",
    "jest-enzyme": "^4.0.2",
    "pre-commit": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "wdio-allure-reporter": "^0.1.2",
    "wdio-cucumber-framework": "^1.0.2",
    "wdio-selenium-standalone-service": "0.0.9",
    "wdio-spec-reporter": "^0.1.3",
    "webdriverio": "^4.9.11"
  },
  "dependencies": {
    "d2": "^28.3.0",
    "d2-manifest": "^1.0.0",
    "d2-ui": "^28.0.8",
    "d2-utilizr": "^0.2.15",
    "i18next": "^10.2.1",
    "i18next-xhr-backend": "^1.5.0",
    "loglevel": "^1.6.0",
    "material-ui": "^0.20.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-i18next": "^7.3.0",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.0.17",
    "rxjs": "^5.5.5"
  },
  "scripts": {
    "prestart": "npm run manifest",
    "start": "react-scripts start",
    "prebuild": "npm run manifest",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "test-e2e": "wdio ./e2e/config/wdio.conf.js",
    "test-no-iterative": "CI=true react-scripts test --env=jsdom",
    "validate": "npm ls --depth 0",
    "lint": "eslint src/",
    "deploy": "npm run build && mvn clean deploy",
    "coverage": "npm test -- --coverage",
    "manifest": "d2-manifest package.json ./public/manifest.webapp"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test-no-iterative"
  ],
  "manifest.webapp": {
    "name": "DHIS2 Maintenance Data Admin App",
    "description": "DHIS2 Maintenance Data Admin App",
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "DHIS2"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  },
  "proxy": {
    "..": {
      "target": "http://localhost:8080"
    }
  }
}
